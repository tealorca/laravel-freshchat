{
    "name": "tealorca/laravel-freshchat",
    "description": "Freshchat Web Widget for Laravel",
    "keywords": [
        "tealorca",
        "laravel-freshchat"
    ],
    "homepage": "https://github.com/tealorca/laravel-freshchat",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tealorca\\LaravelFreshchat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tealorca\\LaravelFreshchat\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tealorca\\LaravelFreshchat\\LaravelFreshchatServiceProvider"
            ],
            "aliases": {
                "LaravelFreshchat": "Tealorca\\LaravelFreshchat\\LaravelFreshchatFacade"
            }
        }
    }
}
